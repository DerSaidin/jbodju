{
  "manifest_version": 2,

  "name": "jbodju",
  "description": "Helps reading of lojban text by overlaying parse tree from camxes.js.",
  "version": "0.1",

  "content_scripts" : [
    {
      "matches" : [ 
         "*://*.lojban.org/*",
         "*://groups.google.com/*"
      ],
      "css": [ "contentscript.css" ],
      "js" : [ "camxes.js", "contentscript.js" ]
    }
  ]
}